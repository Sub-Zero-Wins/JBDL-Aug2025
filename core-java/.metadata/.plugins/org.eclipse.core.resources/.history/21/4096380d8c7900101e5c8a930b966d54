package inheritance;

class A
{
	int x=10;
	static int y=20;
	
	public void m1() {
		System.out.println("non static method");
	}
	public static void m2() {
		System.out.println("static method");
	}
}
class B extends A
{
	
	
}


public class Demo1 
{
	public static void main(String[] args) 
	{
		B.y=30;
		B.m2();
		B b = new B();
		b.x=20;
		b.m1();
		
		
		
		
	}

}
