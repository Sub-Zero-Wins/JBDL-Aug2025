package multithreading;


class BookTicket
{
	int seatNumber=3;
	
	public void bookSeat()
	{
		System.out.println(Thread.currentThread().getName()+" has selected seat"+seatNumber);
		Thread.sleep(2000);
		System.out.println(Thread.currentThread().getName()+" is making payment for"+seatNumber);
		Thread.sleep(2000);
		System.out.println(Thread.currentThread().getName()+" payment successful booked :"+seatNumber);
	}
}

public class TicketBookingDemo 
{
	
	
	
	

}
