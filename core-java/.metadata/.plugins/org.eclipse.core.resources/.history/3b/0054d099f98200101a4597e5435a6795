package lambdaExpressions;

@FunctionalInterface
interface Calc
{
	 int square(int x);
}
class CalcImpl implements Calc
{
	@Override
	public int square(int x) {
		return x*x;		
	}	
}
public class Demo1 
{
	public static void main(String[] args) {
		Calc c = new CalcImpl();
		System.out.println(c.square(10));;
	}

}
