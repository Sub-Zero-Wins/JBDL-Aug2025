package methods;

public class Test 
{
	public static void main(String[] args) //JVM calls main method
	{
		System.out.println("main calc start");//1
		add(); // 2
		sub();
		System.out.println("main calc end");	
	}
	
	public static void add() // method declartion
	{
	   System.out.println("add method");// method body	
	   return;
	}
	
	public static void sub() // method declartion
	{
	   System.out.println("sub method");// method body	
	   return;
	}

}
