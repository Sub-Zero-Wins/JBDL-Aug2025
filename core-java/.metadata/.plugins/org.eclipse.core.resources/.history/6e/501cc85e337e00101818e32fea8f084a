package abstraction;

public class Airport 
{
	public static void main(String[] args) 
	{
		Plane p;
		
		p = new FighterPlane(); // upcasting/loose coupling
		controlPlane(p);
		
		p = new CargoPlane();
		controlPlane(p);
	}
	
	public static void controlPlane(Plane p) 
	{
		p.takeOff();
		p.land();
		
	}

}
