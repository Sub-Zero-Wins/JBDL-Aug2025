package multithreading;

public class Demo1
{
	public static void main(String[] args) 
	{
		int age = 16;
		
		if(age>18)
		{
			System.out.println("allowed to vote");
		}
		
		else {
		
		    try {
		    	throw new InvalidVoterAgeException("not allowed to vote");
			} catch (Exception e) {
				System.out.println(e.getMessage());
			}
		}
		
	}

}
