package methods;

public class Test 
{
	public static void main(String[] args) //JVM calls main method
	{
		System.out.println("main calc start");//1
		int x,y;
		x=10;
		y=20;
		int sum=add(x,y); // 2
		System.out.println("sum of 10+20 ="+sum);
		sub(y,x);
		System.out.println("main calc end");	
	}
	
	public static int add(int x, int y) // method declartion
	{
	   System.out.println("add method");// method body	
	   int sum = x+y;
	   return sum;
	}
	
	public static void sub(int a,int b) // method declartion
	{
	   System.out.println("sub method "+a+"-"+b+"="+(a-b));// method body	
	 
	}

}
