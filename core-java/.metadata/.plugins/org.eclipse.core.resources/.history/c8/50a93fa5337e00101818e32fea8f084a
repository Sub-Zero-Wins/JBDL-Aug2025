package abstraction;

public class Airport 
{
	public static void main(String[] args) 
	{
		Plane p;
		
		p = new FighterPlane(); // upcasting/loose coupling code reusability
		controlPlane(p);
		p.takeOff();
		
		p = new CargoPlane();
		controlPlane(p);
		p.takeOff();
	}
	
	public static void controlPlane(Plane p) 
	{
		p.takeOff();
		p.land();	
	}

}
