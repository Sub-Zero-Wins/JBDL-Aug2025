package oops;

public class Demo3 
{
	public static void main(String[] args)
	{
		String s = "RajaRamMohanRoy";
		
		System.out.println(s.concat(" hello"));
		System.out.println(s);
		System.out.println(s.toLowerCase());
		System.out.println(s.toUpperCase());
		System.out.println(s.startsWith("Raja"));
		System.out.println(s.substring(7));
		System.out.println(s.substring(7,12));
		System.out.println(s.charAt(0));
		System.out.println(s.charAt(1));
		
		
		StringBuilder sbd = new StringBuilder("Rushda");
		
		System.out.println(sbd.append(" Hello"));
		System.out.println(sbd.reverse());
		
		
		
	}

}
