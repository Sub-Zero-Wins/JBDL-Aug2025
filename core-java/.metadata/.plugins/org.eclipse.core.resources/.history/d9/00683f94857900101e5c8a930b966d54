package blocks;

public class Demo 
{
	
	int x;
	static double y;
	
	Demo()
	{
		this.x=10;
	}
	
	static {
		y=20;
	}
	
	{
		x=30;
	}
	
	public void m1() {
		System.out.println("non static method");
	}
	
	public static void m2() {
		System.out.println("static method");
	}
     
	public static void main(String[] args) 
	{
		System.out.println("main start");
		
		System.out.println("main end");
		
	}
}
