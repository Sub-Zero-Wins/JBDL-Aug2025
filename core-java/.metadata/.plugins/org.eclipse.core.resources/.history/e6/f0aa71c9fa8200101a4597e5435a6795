package lambdaExpressions;

@FunctionalInterface
interface Calc
{
	 double PI=3.14;
	
	 String square(int x,String y);
	
}

public class Demo1 
{
	public static void main(String[] args) {
		
		Calc c = (x,y)->{
			
			return x+y; 
		};
		System.out.println(Calc.PI);
		
		System.out.println(c.square(10,"hello"));;
	}

}
