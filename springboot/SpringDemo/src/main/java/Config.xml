<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	
	<bean id="addr" class="com.demo.Address" scope="prototype">
        <property name="pin" value="650012" />
		<property name="street" value="Hairan gali ,Pareshan Road" />
		<property name="city" value="Hyderabad" />
	</bean>

	<bean id="emp" class="com.demo.Employee"  scope="prototype">
       	<property name="id" value="101" />
		<property name="name" value="saqib" />
		<property name="addr" ref="addr" />
	</bean>
	
	

	<!-- more bean definitions go here -->

</beans>









